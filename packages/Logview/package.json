    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-refresh": "^0.16.0",
    "stylelint": "^15.0.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
