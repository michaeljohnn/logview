    "lucide-react": "^0.479.0",
    "react-i18next": "^15.4.0",
    "split.js": "^1.6.5"
  },
  "devDependencies": {
    "@rspack/cli": "^1.2.7",
    "@rspack/core": "^1.2.7",
    "@rspack/plugin-react-refresh": "^1.0.1",
    "@types/ansi-html": "^0.0.0",
    "@types/js-yaml": "^4.0.9",
